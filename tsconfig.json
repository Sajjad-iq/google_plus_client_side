{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": [
      "vite-plugin-pwa/client"
    ]
  },
  "include": [
    "src",
    "src/Components/shared/NotificationsCard/styled/.styled.tsx",
    "src/Components/shared/PostPreviewWindow/Components/OptionButtonAndOptionsWindow/styled/BorderButton.styled.tsx"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}